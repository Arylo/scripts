{
  "name": "Add Font Family for the Self-managed Gitlab",
  "name:zh": "为自托管 Gitlab 添加字体",
  "include": [
    "/^https:\/\/git\\b.[^\/]+\/.*\/-\/raw\/.*/",
    "/^https:\/\/git\\b.[^\/]+\/.*\/-\/blob\/.*/",
    "/^https:\/\/git\\b.[^\/]+\/.*\/-\/commit\/.*/",
    "/^https:\/\/git\\b.[^\/]+\/.*\/-\/merge_requests\/(\\d+|new)\\b/",
    "/^https:\/\/git\\b.[^\/]+\/.*\/-\/merge_requests\/(\\d+|new)\/diffs\\b/"
  ],
  "run-at": "document-end",
  "grant": "GM_addStyle"
}

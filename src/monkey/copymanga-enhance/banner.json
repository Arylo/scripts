{
  "name": "快乐看拷贝",
  "name:en": "Enhance the copy manga site",
  "description": "对开布局、支持带鱼屏、自适应图片高度、快捷翻页、支持键盘操作",
  "description:en": "support ultra-wide screen, adaptive image height, quick page turning, keyboard operation",
  "match": [
    "https://copymanga.com/comic/*/chapter/*",
    "https://*.copymanga.com/comic/*/chapter/*",
    "https://copymanga.org/comic/*/chapter/*",
    "https://*.copymanga.org/comic/*/chapter/*",
    "https://copymanga.net/comic/*/chapter/*",
    "https://*.copymanga.net/comic/*/chapter/*",
    "https://copymanga.info/comic/*/chapter/*",
    "https://*.copymanga.info/comic/*/chapter/*",
    "https://copymanga.site/comic/*/chapter/*",
    "https://*.copymanga.site/comic/*/chapter/*",
    "https://copymanga.tv/comic/*/chapter/*",
    "https://*.copymanga.tv/comic/*/chapter/*",
    "https://mangacopy.com/comic/*/chapter/*",
    "https://*.mangacopy.com/comic/*/chapter/*",
    "https://copy-manga.com/comic/*/chapter/*",
    "https://*.copy-manga.com/comic/*/chapter/*",
    "https://copy20.com/comic/*/chapter/*",
    "https://*.copy20.com/comic/*/chapter/*",
    "https://copy2000.site/comic/*/chapter/*",
    "https://*.copy2000.site/comic/*/chapter/*",
    "https://2025copy.com/comic/*/chapter/*",
    "https://*.2025copy.com/comic/*/chapter/*"
  ],
  "connect": "unpkg.com",
  "require": "https://unpkg.com/vue@3/dist/vue.global.prod.js",
  "resource": "vue https://unpkg.com/vue@3/dist/vue.global.prod.js",
  "run-at": "document-end",
  "grant": [
    "GM_getResourceText",
    "GM_addStyle",
    "GM_setValue",
    "GM_getValue"
  ]
}
